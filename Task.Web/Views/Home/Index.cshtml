@model List<FileModel>

<center>
<h1>Files</h1>
</center>

<table class="table">
    <thead>
        <tr>
            <th>File Name</th>
            <th>Upload Time</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var file in Model)
        {
            <tr>
                <td>@file.FileName</td>
                <td>@file.UploadTime</td>
                <td>
                    <a asp-action="DownloadFile" asp-route-id="@file.Id">Download</a>
                </td>
            </tr>
        }
    </tbody>
</table>
